    <cells xsi:type="pcmmm:Extra" name="34,2: an inning is one period of batting for each team (" verbatim="an inning is one period of batting for each team (3 outs per half-inning)" row="34" rowspan="1" column="2" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="35,0: lineup" verbatim="lineup" row="35" rowspan="1" colspan="1"/>
