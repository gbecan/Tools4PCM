    <cells xsi:type="pcmmm:Extra" name="32,2: powerfully hit ball, often used to describe when t" verbatim="powerfully hit ball, often used to describe when the ball comes off the bat fast and flat &quot;Line Drive&quot; (could be a hit, or caught for an out)" row="32" rowspan="1" column="2" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="33,0: infield" verbatim="infield" row="33" rowspan="1" colspan="1"/>
