    <cells xsi:type="pcmmm:Extra" name="1,1: capital gains, dividends, and interest within acco" verbatim="Capital gains, dividends, and interest within account incur no tax liability." row="1" rowspan="1" column="1" colspan="4"/>
    <cells xsi:type="pcmmm:Header" name="2,0: subjected taxes" verbatim="Subjected taxes" row="2" rowspan="1" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="2,1: contributions are usually pre-tax; but can also be" verbatim="Contributions are usually pre-tax; but can also be post-tax, if allowed by plan. Distributions are taxed as ordinary income (except any post-tax principal)." row="2" rowspan="1" column="1" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="2,2: contributions are post-tax. qualified distribution" verbatim="Contributions are post-tax. Qualified distributions are not taxable." row="2" rowspan="1" column="2" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="2,3: contributions are deductible (subject to condition" verbatim="Contributions are deductible (subject to conditions). When deducted, contributions are pre-tax, otherwise, they are post-tax.  Distributions are taxed as ordinary income (except any non-deducted principal)." row="2" rowspan="1" column="3" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="2,4: contributions are post-tax. qualified distribution" verbatim="Contributions are post-tax. Qualified distributions are not taxable." row="2" rowspan="1" column="4" colspan="1"/>
    <cells xsi:type="pcmmm:Header" name="3,0: employer or individual" verbatim="Employer or Individual" row="3" rowspan="1" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="3,1: employer or sole proprietor sets up this plan" verbatim="Employer or Sole Proprietor sets up this plan" row="3" rowspan="1" column="1" colspan="2"/>
    <cells xsi:type="pcmmm:Extra" name="3,3: individual sets up this plan" verbatim="Individual sets up this plan" row="3" rowspan="1" column="3" colspan="2"/>
    <cells xsi:type="pcmmm:Header" name="4,0: contribution limits" verbatim="Contribution Limits" row="4" rowspan="1" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="4,1: employee contribution limit of $17.5k/yr for under" verbatim="Employee contribution limit of $17.5k/yr for under 50 in 2013; $23k/yr for age 50 or above; limits are a total of pre-tax Traditional 401(k) and Roth 401(k) contributions. Total employee (including after-tax Traditional 401(k)) and employer combined contributions must be lesser of 100% of employee's salary or $51k ($56.5k for age 50 or above). There is no income cap for this investment class." row="4" rowspan="1" column="1" colspan="2"/>
    <cells xsi:type="pcmmm:Extra" name="4,3: $5.5k/yr for age 49 or below; $6.5k/yr for age 50 " verbatim="$5.5k/yr for age 49 or below; $6.5k/yr for age 50 or above in 2013; limits are total for traditional IRA and Roth IRA contributions combined. Cannot contribute more than annual earned income. For direct contributions to Roth IRAs, contribution limit is reduced in a &quot;phase-out&quot; range, for single AGI>$112k and joint AGI>$178k Contribution limit does not apply to conversions from traditional IRA to Roth IRA." row="4" rowspan="1" column="3" colspan="2"/>
    <cells xsi:type="pcmmm:Header" name="5,0: contribution notes" verbatim="Contribution notes" row="5" rowspan="1" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="5,1: " verbatim="" row="5" rowspan="1" column="1" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="5,2: effective limit is higher compared to traditional " verbatim="Effective limit is higher compared to traditional 401k as the contributions are post-tax." row="5" rowspan="1" column="2" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="5,3: " verbatim="" row="5" rowspan="1" column="3" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="5,4: effective limit is higher compared to traditional " verbatim="Effective limit is higher compared to traditional IRA as the contributions are post-tax." row="5" rowspan="1" column="4" colspan="1"/>
    <cells xsi:type="pcmmm:Header" name="6,0: matching contributions" verbatim="Matching Contributions" row="6" rowspan="1" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="6,1: matching contributions available from some employe" verbatim="Matching contributions available from some employers." row="6" rowspan="1" column="1" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="6,2: matching contributions available through some empl" verbatim="Matching contributions available through some employers, but they must sit in a pretax account" row="6" rowspan="1" column="2" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="6,3: no matching contributions available" verbatim="No matching contributions available" row="6" rowspan="1" column="3" colspan="2"/>
    <cells xsi:type="pcmmm:Header" name="7,0: deduction limits" verbatim="Deduction Limits" row="7" rowspan="1" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="7,1: generally no limit on the amount deductible from i" verbatim="Generally no limit on the amount deductible from income, but somewhat complicated due to HCE (highly compensated employees) rules" row="7" rowspan="1" column="1" colspan="2"/>
    <cells xsi:type="pcmmm:Extra" name="7,3: full deduction available on incomes up to $188k, d" verbatim="Full deduction available on incomes up to $188k, depending on tax filing status. See full rules." row="7" rowspan="1" column="3" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="7,4: tax-exempt earnings on contributions available up " verbatim="Tax-exempt earnings on contributions available up to incomes of $173k, depending on tax filing status. See full rules and Backdoor Roth IRA Contributions." row="7" rowspan="1" column="4" colspan="1"/>
    <cells xsi:type="pcmmm:Header" name="8,0: " verbatim="" row="8" rowspan="1" colspan="1"/>
    <cells xsi:type="pcmmm:Header" name="8,1: (traditional) 401(k)" verbatim="(Traditional) 401(k)" row="8" rowspan="1" column="1" colspan="1"/>
