    <cells xsi:type="pcmmm:Extra" name="3,2: in june, 2006 mckinney voted for network neutralit" verbatim="In June, 2006 McKinney voted for network neutrality." row="3" rowspan="1" column="2" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="3,3: strongly supports net neutrality." verbatim="Strongly supports net neutrality." row="3" rowspan="1" column="3" colspan="1"/>
  </matrices>
  <matrices name="Lobbying">
    <cells xsi:type="pcmmm:Header" name="0,0: john mccain" verbatim="John McCain" rowspan="1" colspan="2"/>
