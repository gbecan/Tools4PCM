  <matrices name="Foreign aid">
    <cells xsi:type="pcmmm:Header" name="0,0: john mccain" verbatim="John McCain" rowspan="1" colspan="2"/>
    <cells xsi:type="pcmmm:Header" name="0,2: barack obama" verbatim="Barack Obama" rowspan="1" column="2" colspan="2"/>
    <cells xsi:type="pcmmm:Extra" name="1,0: though john mccain plans to expand foreign aid, sp" verbatim="Though John McCain plans to expand foreign aid, specifically targeting malaria in Africa, he has expressed concern that too much American aid money is embezzled or outright stolen by corrupt foreign governments." row="1" rowspan="1" colspan="2"/>
    <cells xsi:type="pcmmm:Extra" name="1,2: obama would double foreign aid to $50&#xa0;billion by 2" verbatim="Obama would double foreign aid to $50&#xa0;billion by 2012." row="1" rowspan="1" column="2" colspan="2"/>
    <cells xsi:type="pcmmm:Header" name="2,0: chuck baldwin" verbatim="Chuck Baldwin" row="2" rowspan="1" colspan="1"/>
    <cells xsi:type="pcmmm:Header" name="2,1: bob barr" verbatim="Bob Barr" row="2" rowspan="1" column="1" colspan="1"/>
