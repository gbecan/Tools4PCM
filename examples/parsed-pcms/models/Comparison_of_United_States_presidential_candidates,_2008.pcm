    <cells xsi:type="pcmmm:Extra" name="1,2: obama does not take contributions from federally r" verbatim="Obama does not take contributions from federally registered lobbyists or PACs, though he does accept money from non-federal lobbyists and unregistered lobbyists." row="1" rowspan="1" column="2" colspan="2"/>
    <cells xsi:type="pcmmm:Header" name="2,0: chuck baldwin" verbatim="Chuck Baldwin" row="2" rowspan="1" colspan="1"/>
