    <cells xsi:type="pcmmm:Header" name="2,3: ralph nader" verbatim="Ralph Nader" row="2" rowspan="1" column="3" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="3,0: supports the right to bear arms as an individual r" verbatim="Supports the right to bear arms as an individual right." row="3" rowspan="1" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="3,1: supports the right to bear arms as an individual r" verbatim="Supports the right to bear arms as an individual right. &quot;I oppose any law requiring registration of, or restricting the ownership, manufacture, or transfer or sale of firearms or ammunition to law-abiding citizens.&quot;" row="3" rowspan="1" column="1" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="3,2: " verbatim="" row="3" rowspan="1" column="2" colspan="1"/>
