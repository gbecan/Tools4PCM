    <cells xsi:type="pcmmm:Extra" name="1,2: barack obama was 1 of 22 senators to vote against " verbatim="Barack Obama was 1 of 22 Senators to vote against the appointment of Justice Roberts." row="1" rowspan="1" column="2" colspan="2"/>
