    <cells xsi:type="pcmmm:Extra" name="16,0: direct fatalities from the accident" verbatim="Direct fatalities from the accident" row="16" rowspan="1" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="16,1: 2 crew members (gone to inspect the buildings imme" verbatim="2 crew members (gone to inspect the buildings immediately after the earthquake and before the tsunami) due to drowning" row="16" rowspan="1" column="1" colspan="1"/>
    <cells xsi:type="pcmmm:Extra" name="16,2: 31 (64 confirmed deaths from radiation as of 2008," verbatim="31 (64 confirmed deaths from radiation as of 2008, according to the UN)" row="16" rowspan="1" column="2" colspan="1"/>
